{
  "name": "keypress-overlay",
  "version": "0.1.0",
  "main": "./public/main.js",
  "node-remote": "http://localhost:3000",
  "homepage": ".",
  "build": {
    "nwVersion": "0.55.0",
    "excludes": [
      "**/bin",
      "**/bin/**/*"
    ]
  },
  "scripts": {
    "start": "npm run nwjs-dev",
    "react:start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "nwjs-react": "cross-env BROWSER=none npm run react:start",
    "nwjs-run": "wait-on http://localhost:3000 && cross-env NWJS_START_URL=http://localhost:3000 ./bin/nwjs-sdk-v0.55.0-win-x64/nw .",
    "nwjs-dev": "concurrently \"npm run nwjs-react\" \"npm run nwjs-run\"",
    "prenwjs-reactbuild": "npm run build",
    "nwjs-reactbuild": "./bin/nwjs-sdk-v0.55.0-win-x64/nw .",
    "prenwjs-pack": "rimraf ./dist && npm run build",
    "nwjs-pack": "build --tasks win-x64 --mirror https://dl.nwjs.io/ ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.20",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.3.5",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "nwjs-builder-phoenix": "^1.15.0",
    "wait-on": "^6.0.0"
  }
}
